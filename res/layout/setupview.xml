<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen  
    xmlns:android="http://schemas.android.com/apk/res/android">  
	<PreferenceCategory android:title="Wifi">
		<EditTextPreference  
			android:key="ssidpref"  
			android:title="Change SSID:"  
			android:summary="Click to pop up a field for entry"  
			android:dialogTitle="New SSID:"  
		/>      
		<ListPreference  
			android:key="channelpref"  
			android:title="Change Channel:"  
			android:summary="Click to pop up a list to choose from"  
			android:entries="@array/channelnames"  
			android:entryValues="@array/channelvalues"  
			android:dialogTitle="New Channel:" /> 
		<ListPreference  
			android:key="powermodepref"  
			android:title="Change Powermode:"  
			android:summary="Click to pop up a list to choose from"  
			android:entries="@array/powermodenames"  
			android:entryValues="@array/powermodevalues"  
			android:dialogTitle="New Powermode:" /> 			
	</PreferenceCategory>
	<PreferenceCategory android:title="Phone">
		 <CheckBoxPreference  
			android:key="syncpref"  
			android:title="Disable Auto-Sync"  
			android:summary="Check it on, check it off" />  
	</PreferenceCategory>
</PreferenceScreen>  
